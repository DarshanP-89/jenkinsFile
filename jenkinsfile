pipeline{
    agent any
    tools {
        maven 'MAVEN' 
    }
    stages{
        stage("Test"){
            steps{
                bat "mvn --version"
                echo "====executing A===="
            }
        }
        stage("Build"){
            steps{
                echo "====executing A===="
            }
        }
        stage("Deploy on test"){
            steps{
                echo "====executing A===="
            }
        }
        stage("Deploy on prod"){
            steps{
                echo "====executing A===="
            }
        }
    }

    post{
        always{
            echo "========always========"
	}
        success{
            echo "========pipeline executed successfully ========"
        }
        failure{
            echo "========pipeline execution failed========"
        }
    }
}