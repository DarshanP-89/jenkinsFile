pipeline{
    agent any
    stages{
        stage("Test"){
            steps{
                echo "====executing A===="
            }
        }
        stage("Build"){
            steps{
                echo "====executing A===="
                bat "docker build -t myimg ."
            }
        }
        stage("Deploy on test"){
            steps{
                echo "====executing A===="
                
                bat "docker tag myimg darshan8998/custom-image"
                bat "docker login -u darshan8998 -p dckr_pat_q0Ui5H5k3_3Nn5DrL8U3t_XVT7o"
                bat "docker push darshan8998/custom-image"
            }
        }
        stage("Deploy on prod"){
            steps{
                echo "====executing A===="
            }
        }
    }

    post{
        always{
            echo "========always========"
	}
        success{
            echo "========pipeline executed successfully ========"
        }
        failure{
            echo "========pipeline execution failed========"
        }
    }
}